
@{
    Layout = "";
}
<html>
<head>
    <title>Iniciar Session</title>
    <meta http-equiv="Content-type" content="text/html; charset=utf-8" />
    <link rel="stylesheet" href="~/Content/css/style.css" type="text/css" media="all" />
    <!--[if lte IE 6]><link rel="stylesheet" href="~/Content/ie6.css" type="text/css" media="all" /><![endif]-->
    <script src="~/Content/Js/jquery-1.4.1.min.js" type="text/javascript"></script>
    <script src="~/Content/Js/jquery-func.js" type="text/javascript"></script>
    <script src="~/Content/Js/jquery.jcarousel.pack.js" type="text/javascript"></script>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/font-awesome/css/font-awesome.css" rel="stylesheet">

    <!-- Page-Level Plugin CSS - Blank -->
    <!-- SB Admin CSS - Include with every page -->
    <link href="~/Content/css/sb-admin.css" rel="stylesheet">

    <script src="~/Content/js/jquery-1.10.2.js"></script>
    <script src="~/Content/js/bootstrap.min.js"></script>
    <script src="~/Content/js/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="~/Content/js/sb-admin.js"></script>
 


    <!-- SB Admin Scripts - Include with every page -->
    <script src="~/Content/js/sb-admin.js"></script>
</head>
<body>
    <div class="shell">
        <div id="header">
            <div id="navigation">
                <ul>
                    <li><a href="@Url.Action("Index", "Home")">Inicio</a></li>
                </ul>
            </div>
            <!-- Cart -->

        </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-md-4 col-md-offset-4">
                <div class="login-panel panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title">Please Sign In</h3>
                    </div>
                    <div class="panel-body">
                        <form role="form">
                            <fieldset>
                                <div class="form-group">
                                    <input class="form-control" placeholder="E-mail" name="email" type="email" autofocus>
                                </div>
                                <div class="form-group">
                                    <input class="form-control" placeholder="Password" name="password" type="password" value="">
                                </div>
                                <div class="checkbox">
                                    <label>
                                        <input name="remember" type="checkbox" value="Remember Me">Remember Me
                                    </label>
                                </div>
                                <!-- Change this to a button or input when using this as a form -->
                                <a href="@Url.Action("Index", "Home")" class="btn btn-lg btn-success btn-block">Login</a>
                                <a <link  href="@Url.Action("Registro", "Usuario")"  />No tienes cuenta? Registrate aqui </a>
                            </fieldset>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
