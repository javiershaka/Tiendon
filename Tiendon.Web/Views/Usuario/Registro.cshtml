</head>
<body>

    <div id="header">
        <h1 id="logo"><a href="#">shoparound</a></h1>
        <!-- Cart -->
        <div id="cart">
            <a href="#" class="cart-link">Your Shopping Cart</a>
            <div class="cl">&nbsp;</div>
            <span>Articles: <strong>4</strong></span> &nbsp;&nbsp; <span>Cost: <strong>$250.99</strong></span>
        </div>
        <!-- End Cart -->
        <!-- Navigation -->
        <div id="navigation">
            <ul>
                <li><a href="#" class="active">Home</a></li>
                <li><a href="#">Support</a></li>
                <li><a href="#">My Account</a></li>
                <li><a href="#">The Store</a></li>
                <li><a href="#">Contact</a></li>
            </ul>
        </div>
        <!-- End Navigation -->
    </div>
    <div class="panel-body">
        <div class="row">
            <div class="col-lg-6">
                <form role="form" method="post">
                    <div class="form-group">
                        <label>Nombre de usuario</label>
                        <input placeholder="javiershaka" class="form-control txtForm">


                    </div>
                    <div class="form-group">
                        <label>Correo</label>

                        <input placeholder="Example@gmail.com" class="form-control txtForm">


                    </div>
                    <div class="form-group">
                        <label>Password</label>
                        <input type="password" placeholder="******" class="form-control txtForm">


                    </div>
                    <div class="form-group">
                        <label>Verificar password</label>
                        <input type="password" placeholder="******" class="form-control txtForm">


                    </div>
                    <div class="form-group">
                        <label>Edad</label>
                        <input placeholder="22" class="form-control txtForm">


                    </div>
                    <div class="form-group">
                        <select name="genero">
                            <option value="Mujer">Mujer</option>
                            <option value="Hombre">Hombre</option>

                        </select>

                    </div>
                    <div class="form-group">
                        <label>Direccion</label>
                        <input placeholder="calle 20 avenidad 18" class="form-control txtForm">


                    </div>
                    <input type="submit" class="btn btn-success" />
                </form>
            </div>
        </div>
    </div>

</body>
